{% extends layout_inscription %}
{% block title %}Première inscription{% endblock %}
{% block content %}
<div class="main-container">
    <!-- hidden Title H1 -->
    <h1 class="hide">Dossier etudiant - première inscription</h1>

    <div class="main-container profil-container">
        <div class="container">
            <div class="card">
                <div class="card-body">
                    <h2>Fiche de renseignements</h2>
                    <span class="error">{{ form_errors(registrationForm) }}</span>
                    {{ form_start(registrationForm, {'attr': {'class': 'row'}}) }}
                    {{ form_row(registrationForm._token) }}
                        <div class="form-bloc">
                            <div class="row">
                                <div class="form-group col-md-6">
                                    <label class="control-label" for="first_inscription_form_payementRef">Référence de paiement <span class="required">*</span></label>
                                    {{ form_widget(registrationForm.payementRef) }}
                                    <span class="error">{{ form_errors(registrationForm.payementRef) }}</span>
                                </div>



                                <div class="form-group col-md-3">
                                    <label class="control-label" for="first_inscription_form_montant">Montant <span class="required">*</span></label>
                                    {{ form_widget(registrationForm.montant) }}
                                    <span class="error">{{ form_errors(registrationForm.montant) }}</span>
                                </div>

                                <div class="form-group col-md-3">
                                    <label class="control-label" for="first_inscription_form_mode_paiement">Mode de paiement</label>
                                    {{ form_widget(registrationForm.mode_paiement) }}
                                    <span class="error">{{ form_errors(registrationForm.mode_paiement) }}</span>
                                </div>



                                <div class="form-group col-md-6">
                                    <label class="control-label" for="first_inscription_form_payementRefDate">Date <span class="required">*</span></label>
                                    {{ form_widget(registrationForm.payementRefDate) }}
                                    <span class="error">{{ form_errors(registrationForm.payementRefDate) }}</span>
                                </div>
                                <div class="form-group col-md-6">
                                    <label class="control-label" for="first_inscription_form_payementRefPath">Scan paiement <span class="required">*</span></label>
                                    {{ form_widget(registrationForm.payementRefPath) }}
                                    <span class="error">{{ form_errors(registrationForm.payementRefPath) }}</span>
                                </div>


                                


                                <div class="form-group col-md-4">
                                    <label class="control-label" for="first_inscription_form_mention">Mention <span class="required">*</span></label>
                                    <select id="first_inscription_form_mention" name="first_inscription_form[mention]" required="required" class="form-control">
                                        {% for mention in mentions %}
                                            <option value="{{mention.id}}">{{ mention.nom }}</option>
                                        {% endfor %}
                                    </select>
                                </div>

                                <div class="form-group col-md-4">
                                    <label class="control-label" for="niveau">Niveau <span class="required">*</span></label>
                                    <select id="first_inscription_form_niveau" name="first_inscription_form[niveau]" required="required" class="form-control">
                                    {% for niveau in niveaux %}
                                            <option value="{{niveau.id}}">{{ niveau.libelle }}</option>
                                        {% endfor %}
                                    </select>
                                </div>

                                <div class="form-group col-md-4">
                                    <label class="control-label" for="first_inscription_form_parcours">Parcours</label>
                                    <select id="first_inscription_form_parcours" name="first_inscription_form[parcours]" class="form-control">
                                        {% if parcours | length > 0  %}
                                            {% for parc in parcours %}
                                                <option value="{{parc.id}}">{{ parc.nom }}</option>
                                            {% endfor %}
                                        {% endif %}
                                    </select>
                                </div>
                                
                                <div class="form-group col-md-2">
                                    <label class="control-label" for="first_inscription_form_civility">Civilité</label>
                                    {{ form_widget(registrationForm.civility) }}
                                    <span class="error">{{ form_errors(registrationForm.civility) }}</span>
                                </div>
                                <div class="form-group col-md-5">
                                    <label class="control-label" for="first_inscription_form_lastName">Nom(s) <span class="required">*</span></label>
                                    {{ form_widget(registrationForm.lastName) }}
                                    <span class="error">{{ form_errors(registrationForm.lastName) }}</span>
                                </div>
                                <div class="form-group col-md-5">
                                    <label class="control-label" for="first_inscription_form_firstName">Prénom(s)</label>
                                    {{ form_widget(registrationForm.firstName) }}
                                    <span class="error">{{ form_errors(registrationForm.firstName) }}</span>
                                </div>
                                <div class="form-group col-md-2">
                                    <label class="control-label" for="dateOfBirth">Date de naissance <span class="required">*</span></label>
                                    {{ form_widget(registrationForm.birthDate) }}
                                    <span class="error">{{ form_errors(registrationForm.birthDate) }}</span>
                                </div>
                                <div class="form-group col-md-5">
                                    <label class="control-label" for="first_inscription_form_birthPlace">Lieu de naissance</label>
                                    {{ form_widget(registrationForm.birthPlace) }}
                                    <span class="error">{{ form_errors(registrationForm.birthPlace) }}</span>
                                </div>

                                <div class="form-group col-md-5">
                                    <label class="control-label" for="first_inscription_form_nationality">Nationalité</label>
                                    {{ form_widget(registrationForm.nationality) }}
                                </div>

                                <!-- Pièces fournies -->
                                <h4 class="text-left text-dark">Pièces fournies</h4>

                                <div class="form-group col-md-3">
                                    {{ form_label(registrationForm.lettreMotivation, 'Lettre de motivation', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.lettreMotivation) }}
                                </div>
                                <div class="form-group col-md-5">
                                    {{ form_label(registrationForm.lettrePresentation, 'Lettre de présentation d\'une autorité eclésiastique ou d\'une supérieur(e)', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.lettrePresentation) }}
                                </div>
                                <div class="form-group col-md-4">
                                    {{ form_label(registrationForm.cv, 'CV', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.cv) }}
                                </div>
                                <div class="form-group col-md-4">
                                    <label class="control-label" for="first_inscription_form_photo1">Photo d'identité</label>
                                    {{ form_widget(registrationForm.photo1) }}
                                </div>
                                <!-- <div class="form-group col-md-3">
                                    <label class="control-label" for="first_inscription_form_photo2">Photo d'identité 2</label>
                                    {{ form_widget(registrationForm.photo2) }}
                                </div> -->
                                <div class="form-group col-md-4">
                                    {{ form_label(registrationForm.certMedical, 'Certificat medical d\'aptitude', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.certMedical) }}
                                </div>
                                <div class="form-group col-md-4">
                                    {{ form_label(registrationForm.acteNaissance, 'Copie d\'acte de naissance', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.acteNaissance) }}
                                </div>

                                <div class="form-group col-md-4">
                                    <label class="control-label" for="first_inscription_form_baccFile">Diplôme ou attestation ou relevé des notes du baccalauréat</label>
                                    {{ form_widget(registrationForm.baccFile) }}
                                    <span class="error">{{ form_errors(registrationForm.baccFile) }}</span>
                                </div>
                                <div class="form-group col-md-2">
                                    <label class="control-label" for="first_inscription_form_bacc_annee">Année</label>
                                    {{ form_widget(registrationForm.bacc_annee) }}
                                    <span class="error">{{ form_errors(registrationForm.bacc_annee) }}</span>
                                </div>
                                <div class="form-group col-md-2">
                                    <label class="control-label" for="bacc_serie">Série (Baccalaureat Malagasy) &nbsp;</label>
                                    {{ form_widget(registrationForm.baccSerie, {'attr': {'class': 'form-check-inline'}}) }}
                                </div>
                                <div class="form-group col-md-2">
                                    <label class="control-label" for="first_inscription_form_baccAnnee">Numero d'inscription</label>
                                    {{ form_widget(registrationForm.baccNuminscription) }}
                                    <span class="error">{{ form_errors(registrationForm.baccNuminscription) }}</span>
                                </div>
                                <div class="form-group col-md-2">
                                    <label class="control-label" for="first_inscription_form_baccAnnee">Mention</label>
                                    {{ form_widget(registrationForm.baccMention) }}
                                    <span class="error">{{ form_errors(registrationForm.baccMention) }}</span>
                                </div>
                                <div class="form-group col-md-3">
                                    <label class="control-label" for="first_inscription_form_cinFile">Photocopie CIN</label>
                                    {{ form_widget(registrationForm.cinFile) }}
                                </div>
                                <!-- END pièces fournies infos -->

                                 <div class="form-group col-md-3">
                                    <label class="control-label" for="first_inscription_form_cinNum">Numéro CIN</label>
                                    {{ form_widget(registrationForm.cinNum) }}
                                </div>
                                <div class="form-group col-md-3">
                                    <label class="control-label" for="first_inscription_form_cinDeliveryDate">Delivré le</label>
                                    {{ form_widget(registrationForm.cinDeliveryDate) }}
                                    <span class="error">{{ form_errors(registrationForm.cinDeliveryDate) }}</span>
                                </div>
                                <div class="form-group col-md-3">
                                    <label class="control-label" for="first_inscription_form_cinDeliveryLocation">à</label>
                                    {{ form_widget(registrationForm.cinDeliveryLocation) }}
                                </div>
                                <div class="form-group col-md-4">
                                    <label class="control-label" for="first_inscription_form_address">Adresse de l'étudiant </label>
                                    {{ form_widget(registrationForm.address) }}
                                    <span class="error">{{ form_errors(registrationForm.address) }}</span>
                                </div>
                                <div class="form-group col-md-4">
                                    <label class="control-label" for="first_inscription_form_phone">Téléphone de l'étudiant </label>
                                    {{ form_widget(registrationForm.phone) }}
                                    <span class="error">{{ form_errors(registrationForm.phone) }}</span>
                                </div>
                                <div class="form-group col-md-4">
                                    <label class="control-label" for="first_inscription_form_email">Email <span class="required">*</span></label>
                                    {{ form_widget(registrationForm.email, { 'attr': { 'readonly': 'readonly', 'value': email } }) }}
                                    <span class="error">{{ form_errors(registrationForm.email) }}</span>
                                </div>
                                <div class="form-group col-md-4">
                                    <label class="control-label" for="first_inscription_form_religion">Religion <span class="required">*</span></label>
                                    {{ form_widget(registrationForm.religion) }}
                                    <span class="error">{{ form_errors(registrationForm.religion) }}</span>
                                </div>
                                <div class="form-group col-md-4">
                                    <label class="control-label" for="first_inscription_form_sport">Sport pratiqué</label>
                                    {{ form_widget(registrationForm.sport) }}
                                    <span class="error">{{ form_errors(registrationForm.sport) }}</span>
                                </div>
                                <div class="form-group col-md-3">
                                    {{ form_label(registrationForm.situationMatrimoniale, 'Situation matrimoniale', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.situationMatrimoniale) }}
                                </div>

                                <div class="form-group col-md-8">
                                    {{ form_label(registrationForm.autreDocLibelle, 'Autres documents (Titres)', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.autreDocLibelle) }}
                                </div>
                                <div class="form-group col-md-3">
                                    {{ form_label(registrationForm.autreDocFichier, 'Autres documents (Fichiers)', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.autreDocFichier) }}
                                </div>

                                <!-- Filiation -->
                                <h4 class="text-left text-dark">Filiation</h4>
                                <div class="form-group col-md-4">
                                    {{ form_label(registrationForm.fatherName, 'Nom du père', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.fatherName) }}
                                </div>
                                <div class="form-group col-md-8">
