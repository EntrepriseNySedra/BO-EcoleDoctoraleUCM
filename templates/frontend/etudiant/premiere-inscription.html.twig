                                    {{ form_widget(registrationForm.tutorAddressContact) }}
                                </div>
                                <div class="form-group col-md-4">
                                    {{ form_label(registrationForm.tutorJob, 'Profession', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.tutorJob) }}
                                </div>
                                <div class="form-group col-md-8">
                                    {{ form_label(registrationForm.tutorJobAddressContact, 'Adresse et contact', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.tutorJobAddressContact) }}
                                </div>

                                <!-- END Tuteur -->

                                <!-- Conjoint(e) -->
                                <h4 class="text-left text-dark">Pour les mariés (es)</h4>
                                <div class="form-group col-md-3">
                                    {{ form_label(registrationForm.jointLastName, 'Nom du conjoint(e)', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.jointLastName) }}
                                </div>
                                <div class="form-group col-md-3">
                                    {{ form_label(registrationForm.jointFirstName, 'Prénom du conjoint(e)', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.jointFirstName) }}
                                </div>
                                <div class="form-group col-md-6">
                                    {{ form_label(registrationForm.jointAddressContact, 'Adresse et contact', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.jointAddressContact) }}
                                </div>
                                <div class="form-group col-md-4">
                                    {{ form_label(registrationForm.jointJob, 'Profession', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.jointJob) }}
                                </div>
