                                <div class="form-group col-md-8">
                                    {{ form_label(registrationForm.fatherJobAddressContact, 'Adresse et contact', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.fatherJobAddressContact) }}
                                </div>
                                <div class="form-group col-md-4">
                                    {{ form_label(registrationForm.motherName, 'Nom de la mère', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.motherName) }}
                                </div>
                                <div class="form-group col-md-8">
                                    {{ form_label(registrationForm.motherAddressContact, 'Adresse et contact', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.motherAddressContact) }}
                                </div>
                                <div class="form-group col-md-4">
                                    {{ form_label(registrationForm.motherJob, 'Profession du mère', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.motherJob) }}
                                </div>
                                <div class="form-group col-md-8">
                                    {{ form_label(registrationForm.motherJobAddressContact, 'Adresse et contact', { 'label_attr': { 'class': 'control-label' } }) }}
                                    {{ form_widget(registrationForm.motherJobAddressContact) }}
                                </div>

                                <!-- END Filiation -->

                                <!-- Tuteur -->
                                <h4 class="text-left text-dark">Tuteur</h4>
                                <div class="form-group col-md-2">
                                    <label class="control-label" for="first_inscription_form_civility">Civilité</label>
                                    {{ form_widget(registrationForm.tutorCivility) }}
                                    <span class="error">{{ form_errors(registrationForm.tutorCivility) }}</span>
                                </div>
                                <div class="form-group col-md-3">
                                    <label class="control-label" for="first_inscription_form_tutorLastName">Nom(s)</label>
                                    {{ form_widget(registrationForm.tutorLastName) }}
                                </div>
                                <div class="form-group col-md-3">
